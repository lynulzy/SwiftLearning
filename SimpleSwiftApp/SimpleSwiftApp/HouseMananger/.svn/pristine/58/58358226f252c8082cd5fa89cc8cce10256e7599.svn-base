#import "WXBaseModel.h"

@interface ComentModel : WXBaseModel
@property (nonatomic,copy)NSString * say_id;//评论或回复的主键id
@property (nonatomic,copy)NSString * user_id;//评论的用户id
@property (nonatomic,copy)NSString * create_time;//评论或回复的时间
@property (nonatomic,copy)NSString * content;//评论或回复的内容
@property (nonatomic,copy)NSString * flag;//评论或回复的对象的主键id（say_id）
@property (nonatomic,copy)NSString * username;//用户名
